{{ft_sensor}}

def main():

    pi = 3.14159
    home_pos = [0, -pi/2, -pi/2, -pi/2, pi/2, 0]
    movej(home_pos)
    
    ft_thread = run ft_reader()
    count = 0
    while count < 100:
        textmsg(g_g_current_ft)
        count += 1
    global g_ft_reader_stop = True
    join ft_thread
end

main()